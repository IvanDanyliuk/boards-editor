generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("NEXT_PUBLIC_DATABASE_URL")
  directUrl         = env("NEXT_PUBLIC_DIRECT_URL")
}

model User {
  id String @id @unique
  email String
  name String
  image String
  company String
  industry String
  role String
}